<?xml version="1.0" encoding="UTF-8"?>
<clinicaldata:ClinicalData xsi:schemaLocation="Clinicaldata SmartFormCreate1.xsd"
	xmlns:clinicaldata="Clinicaldata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<templateId>SmartFormCreate1</templateId>
	<clientName>String</clientName>
	<clientRequestInitiationTime>${CLIENT_REQUEST_INIT_TIME}</clientRequestInitiationTime>
	<patient>
		<smartFormDocuments>
			<smartFormDocument>
				<documentNativeId>1</documentNativeId>
				<documentUUID>${DOC_UUID}</documentUUID>
				<documentStartDate>
					<literal>${DOC_START_DATE}</literal>
				</documentStartDate>
				<documentEndDate>
					<literal>${DOC_END_DATE}</literal>
				</documentEndDate>
				<patient>
					<identifier>
						<identity>${PATIENT_ID}</identity>
						<assigningFacility>${PATIENT_ASSIGNING_FACILITY}</assigningFacility>
						<assigningAuthority>${PATIENT_ASSIGN_AUTHORITY}</assigningAuthority>
					</identifier>
					<name>
						<prefix>${PATIENT_NAME_PREFIX}</prefix>
						<given>${PATIENT_NAME_GIVEN}</given>
						<middle>${PATIENT_NAME_MIDDLE}</middle>
						<family>${PATIENT_NAME_FAMILY}</family>
						<suffix>${PATIENT_NAME_SUFFIX}</suffix>
						<title>${PATIENT_NAME_TITLE}</title>
					</name>
				</patient>
				<provider>
					<identifier>
						<identity>${PROVIDER_ID}</identity>
						<assigningFacility>${PROVIDER_ASSIGNING_FACILITY}</assigningFacility>
						<assigningAuthority>${PROVIDER_ASSIGNING_AUTHORITY}</assigningAuthority>
					</identifier>
					<name>
						<prefix>${PROVIDER_NAME_PREFIX}</prefix>
						<given>${PROVIDER_NAME_GIVEN}</given>
						<middle>${PROVIDER_NAME_MIDDLE}</middle>
						<family>${PROVIDER_NAME_FAMILY}</family>
						<suffix>${PROVIDER_NAME_SUFFIX}</suffix>
						<title>${PROVIDER_NAME_TITLE}</title>
					</name>
				</provider>
				<facility>
					<identity>${FACILITY_ID}</identity>
					<name>${FACILITY_NAME}</name>
					<assigningAuthority>${FACILITY_ASSIGNING_AUTHORITY}</assigningAuthority>
				</facility>
				<assertions>
					<assertion>
						<assertionUUID>${ASSERTION_UUID}</assertionUUID>
						<seqInDoc>${ASSERTION_SEQ_IN_DOC}</seqInDoc>
						<assertionStartDate>
							<literal>${ASSERTION_START_DATE}</literal>
						</assertionStartDate>
						<assertionEndDate>
							<literal>${ASSERTION_END_DATE}</literal>
						</assertionEndDate>
						<sectionENID>${ASSERTION_SECTION_ENID}</sectionENID>
						<discernibleENID>${ASSERTION_DISCERNIBLE_ENID}</discernibleENID>
						<qualifierENID>${ASSERTION_QUALIFIER_ENID}</qualifierENID>
						<valueENID>${ASSERTION_VALUE_ENID}</valueENID>
					</assertion>
				</assertions>
			</smartFormDocument>
		</smartFormDocuments>
	</patient>
</clinicaldata:ClinicalData>
